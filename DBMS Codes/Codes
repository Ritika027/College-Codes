(1, '2024-05-01', 101),
(2, '2024-05-02', 102);
create view view_patient as
select pid, pname, address from patient;
update view_patient
set address = 'delhi'
where pid = 1;
create view patient_doctor_info as
select p.pname, d.dname, pa.date
from patient p
join patient_admission pa on p.pid = pa.pid
join doctor d on d.did = pa.did;
